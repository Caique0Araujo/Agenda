<div class="sub-container">
  <h1 style="margin-bottom: 5%;">Contatos</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Nome</th>
        <th scope="col">Email</th>
        <th scope="col">Telefone</th>
      </tr>
    </thead>
    <tbody>
      {{#each contacts}}
      <tr>
        <td>{{this.name}}</td>
        <td>{{this.email}}</td>
        <td>{{this.fone}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>